    ],
    // Editor Settings
    "editor.guides.indentation": true,
    "editor.guides.bracketPairs": false,
    "editor.inlineSuggest.enabled": true,
    "editor.hideCursorInOverviewRuler": false,
    "editor.fontFamily": "Dank Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 12.5,
    "editor.fontWeight": "normal",
    "editor.cursorBlinking": "solid",
    "editor.linkedEditing": true,
    "editor.lineHeight": 1.8,
    "editor.lineNumbers": "on",
    "editor.renderWhitespace": "none",
    "editor.hover.enabled": true,
    "editor.scrollbar.horizontalScrollbarSize": 7,
    "editor.scrollbar.verticalScrollbarSize": 7,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.overviewRulerBorder": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.unicodeHighlight.allowedLocales": {
        "es": true
    },
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.matchBrackets": "always",
    "editor.wordSeparators": ",.;{}[]()\":'=",
    "editor.renderLineHighlight": "gutter",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 150,
    "editor.mouseWheelZoom": true,
    "editor.inlayHints.fontSize": 11,
    // Files
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "onFocusChange",
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "files.insertFinalNewline": true,
    // Explorer
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.autoReveal": false,
    "explorer.compactFolders": false,
    // Workbench
    "workbench.tree.indent": 20,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.tabSizing": "fit",
    "workbench.editor.wrapTabs": true,
    "workbench.editor.showIcons": true,
    "workbench.editorAssociations": {
        "*.md": "default",
        "*.html": "default",
        "*.json": "default"
    },
    "workbench.editor.showTabs": "none",
    "workbench.layoutControl.enabled": false,
    "workbench.settings.applyToAllProfiles": [
        "editor.fontFamily",
        "editor.fontSize",
        "editor.fontLigatures"
    ],
    "workbench.iconTheme": "catppuccin-latte",
    "material-icon-theme.hidesExplorerArrows": true,
    "workbench.colorTheme": "Catppuccin Noctis Frapp√©",
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.colorCustomizations": {},
    "terraform.languageServer.args": [
        "serve",
        "/opt/homebrew/bin/terraform"
    ],
    "workbench.activityBar.location": "top",
    "workbench.fontAliasing": "auto",
    // Terminal
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font Mono",
    "terminal.integrated.scrollback": 1000000,
    "terminal.integrated.allowMnemonics": true,
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.ignoreProcessNames": [
        "zsh"
    ],
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontSize": 12.5,
    "terminal.integrated.shellIntegration.enabled": true,
    "terminal.integrated.mouseWheelZoom": true,
    // rego
    "opa.checkOnSave": true,
    "opa.strictMode": false,
    "opa.formatter": "opa-fmt",
    // Python
    "ruff.nativeServer": "on",
    "ruff.lineLength": 100,
    "ruff.showSyntaxErrors": true,
    "ruff.lint.enable": true,
    "ruff.lint.ignore": [],
    "ruff.lint.select": [
        "E4",
        "E7",
        "E9",
        "F",
        "B",
        "Q",
        "ANN", // Type annotation rules
        "PYI", // .pyi file-related rules
        "TCH" // Type-checking related rules
    ],
    "ruff.lint.extendSelect": [
        "W"
    ],
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.fixAll": "always",
            "source.organizeImports": "explicit",
            "source.organizeLinkDefinitions": "always",
            "source.unusedImports": "always"
        }
    },
    "python.analysis.typeCheckingMode": "standard",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.diagnosticSeverityOverrides": {
        "reportMissingModuleSource": "none"
    },
    "python.testing.pytestEnabled": true,
    "python.analysis.typeshedPaths": [
        ".venv/Lib/site-packages"
    ],
    // "[rego]": {
    //     "editor.codeActionsOnSave": {},
    //     "editor.formatOnSave": false,
    //     "editor.defaultFormatter": null
    // },
    // YAML
    "[yaml]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.tabSize": 2
    },
    // Git
    "scm.diffDecorations": "gutter",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.openRepositoryInParentFolders": "always",
    // GitHub
